@article{zhangSynthesizingDoubleHaploid2011,
  title = {Synthesizing Double Haploid Hexaploid Wheat Populations Based on a Spontaneous Alloploidization Process},
  author = {Zhang, Lianquan and Zhang, Li and Luo, Jiangtao and Chen, Wenjie and Hao, Ming and Liu, Baolong and Yan, Zehong and Zhang, Bo and Zhang, Huaigang and Zheng, Youliang and Liu, Dengcai and Yen, Yang},
