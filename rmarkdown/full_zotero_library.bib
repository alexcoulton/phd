@article{kimChromosomeAxisControls2014,
  title = {The {{Chromosome Axis Controls Meiotic Events}} through a {{Hierarchical Assembly}} of {{HORMA Domain Proteins}}},
  author = {Kim, Yumi and Rosenberg, Scott C. and Kugel, Christine L. and Kostow, Nora and Rog, Ofer and Davydov, Vitaliy and Su, Tiffany Y. and Dernburg, Abby F. and Corbett, Kevin D.},
  year = {2014},
  month = nov,
  volume = {31},
  pages = {487--502},
  issn = {1534-5807},
  doi = {10.1016/j.devcel.2014.09.013},
  abstract = {Summary Proteins of the HORMA domain family play central, but poorly understood, roles in chromosome organization and dynamics during meiosis. In Caenorhabditis elegans, four such proteins (HIM-3, HTP-1, HTP-2, and HTP-3) have distinct but overlapping functions. Through combined biochemical, structural, and in~vivo analysis, we find that these proteins form hierarchical complexes through binding of their HORMA domains to cognate peptides within their partners' C-terminal tails, analogous to the ``safety belt'' binding mechanism of Mad2. These interactions are critical for recruitment of HIM-3, HTP-1, and HTP-2 to chromosome axes. HTP-3, in addition to recruiting the other HORMA domain proteins to the axis, plays an independent role in sister chromatid cohesion and double-strand break formation. Finally, we find that mammalian HORMAD1 binds a motif found both at its own C terminus and at that of HORMAD2, indicating that this mode of intermolecular association is a conserved feature of meiotic chromosome structure in eukaryotes.},
  file = {E\:\\Zotero\\storage\\CLCEYGTP\\Kim et al_2014_The Chromosome Axis Controls Meiotic Events through a Hierarchical Assembly of.pdf;E\:\\Zotero\\storage\\BIEINYP9\\S1534580714006194.html},
  journal = {Developmental Cell},
  keywords = {custom},
  number = {4}
}

