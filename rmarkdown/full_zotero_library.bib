@article{hodgkinMeasurementCurrentvoltageRelations1951,
  title = {Measurement of Current-Voltage Relations in the Membrane of the Giant Axon of {{Loligo}}},
  author = {Hodgkin, AL and Huxley, AF and Katz, B},
  year = {1951},
  volume = {116},
