@article{balfourierWorldwidePhylogeographyHistory2019,
  title = {Worldwide Phylogeography and History of Wheat Genetic Diversity},
  author = {Balfourier, Fran{\c c}ois and Bouchet, Sophie and Robert, Sandra and Oliveira, Romain De and Rimbert, H{\'e}l{\`e}ne and Kitt, Jonathan and Choulet, Fr{\'e}d{\'e}ric and Consortium, International Wheat Genome Sequencing and Consortium, BreedWheat and Paux, Etienne},
