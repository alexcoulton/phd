}

@article{dawsonLearningObservationEmerges2013,
  title = {Learning by Observation Emerges from Simple Associations in an Insect Model},
  author = {Dawson, Erika H. and {Avargu{\`e}s-Weber}, Aurore and Chittka, Lars and Leadbeater, Ellouise},
  year = {2013},
  volume = {23},
  pages = {727--730},
  issn = {09609822},
  doi = {10.1016/j.cub.2013.03.035},
  abstract = {Recent debate has questioned whether animal social learning truly deserves the label "social" [1]. Solitary animals can sometimes learn from conspecifics [2, 3], and social learning abilities often correlate with individual learning abilities [4-6], so there may be little reason to view the underlying learning processes as adaptively specialized. Here, we demonstrate how learning by observation, an ability common to primates, birds, rodents, and insects [7-9], may arise through a simple Pavlovian ability to integrate two learned associations. Bumblebees are known to learn how to recognize rewarding flower colors by watching conspecifics from behind a screen [9], and we found that previous associations between conspecifics and reward are critical to this process. Bees that have previously been rewarded for joining conspecifics copy color preferences, but bees that lack such experience do not, and those that associate conspecifics with bitter substances actively avoid those flower colors where others have been seen. Our findings place a seemingly complex social learning phenomenon within a simple associative framework that is common to social and solitary species alike. \textcopyright{} 2013 Elsevier Ltd.},
  file = {E\:\\Zotero\\storage\\JP2JFH84\\Dawson et al. (2013) - bees second order conditioning},
  journal = {Current Biology},
  number = {8},
  pmid = {23562271}
}

