@article{jacobsEmergenceNovelColor2007,
  title = {Emergence of {{Novel Color}}},
  author = {Jacobs, Gerald H. and Williams, Gary A. and Cahill, Hugh and Nathans, Jeremy},
  year = {2007},
  volume = {315},
  pages = {1723--25},
  issn = {0036-8075},
  doi = {10.1126/science.1138838},
  abstract = {Changes in the genes encoding sensory receptor proteins are an essential step in the evolution of new sensory capacities. In primates, trichromatic color vision evolved after changes in X chromosome\textendash linked photopigment genes. To model this process, we studied knock-in mice that expressed a human long-wavelength\textendash sensitive (L) cone photopigment in the form of an X-linked polymorphism. Behavioral tests demonstrated that heterozygous females, whose retinas contained both native mouse pigments and human L pigment, showed enhanced long-wavelength sensitivity and acquired a new capacity for chromatic discrimination. An inherent plasticity in the mammalian visual system thus permits the emergence of a new dimension of sensory experience based solely on gene-driven changes in receptor organization.},
  file = {E\:\\Zotero\\storage\\Z6JR57JH\\1723.full.pdf},
  journal = {Science},
  number = {March},
  pmid = {17379811}
}

