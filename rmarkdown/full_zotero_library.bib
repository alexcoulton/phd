@article{nutmanRapidEmergenceLife2016,
  title = {Rapid Emergence of Life Shown by Discovery of 3,700-Million-Year-Old Microbial Structures},
  author = {Nutman, Allen P. and Bennett, Vickie C. and Friend, Clark R. L. and Van Kranendonk, Martin J. and Chivas, Allan R.},
  year = {2016},
  volume = {537},
  pages = {1--12},
  issn = {0028-0836},
  doi = {10.1038/nature19355},
  abstract = {Biological activity is a major factor in Earth's chemical cycles, including facilitating CO2 sequestration and providing climate feedbacks. Thus a key question in Earth's evolution is when did life arise and impact hydrosphere\textendash atmosphere\textendash lithosphere chemical cycles? Until now, evidence for the oldest life on Earth focused on debated stable isotopic signatures of 3,800\textendash 3,700 million year (Myr)-old metamorphosed sedimentary rocks and minerals from the Isua supracrustal belt (ISB), southwest Greenland. Here we report evidence for ancient life from a newly exposed outcrop of 3,700-Myr-old metacarbonate rocks in the ISB that contain 1\textendash 4-cm-high stromatolites\textemdash macroscopically layered structures produced by microbial communities. The ISB stromatolites grew in a shallow marine environment, as indicated by seawater-like rare-earth element plus yttrium trace element signatures of the metacarbonates, and by interlayered detrital sedimentary rocks with cross-lamination and storm-wave generated breccias. The ISB stromatolites predate by 220 Myr the previous most convincing and generally accepted multidisciplinary evidence for oldest life remains in the 3,480-Myr-old Dresser Formation of the Pilbara Craton, Australia. The presence of the ISB stromatolites demonstrates the establishment of shallow marine carbonate production with biotic CO2 sequestration by 3,700 million years ago (Ma), near the start of Earth's sedimentary record. A sophistication of life by 3,700 Ma is in accord with genetic molecular clock studies placing life's origin in the Hadean eon ( \textbackslash textgreater 4,000 Ma).},
  file = {E\:\\Zotero\\storage\\VSR3ZSVW\\2016 stromatolite.pdf},
  journal = {Nature},
  number = {7621},
  pmid = {27580034}
}

