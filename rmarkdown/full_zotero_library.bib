@article{weigtSolidstemmedSpringWheat2016,
  title = {Solid-Stemmed Spring Wheat Cultivars Give Better Androgenic Response than Hollow-Stemmed Cultivars in Anther Culture},
  author = {Weigt, Dorota and Kiel, Angelika and Nawraca{\l}a, Jerzy and Pluta, Mateusz and {\L}acka, Agnieszka},
  year = {2016},
  volume = {52},
  pages = {619--625},
  issn = {1054-5476},
  doi = {10.1007/s11627-016-9793-2},
  abstract = {Solid-stemmed spring wheat cultivars (Triticum aestivum L.) are resistant to the stem sawfly (Cephus cinctus Nort.) and lodging. Anthers of 24 spring wheat cultivars with varying content of pith in the stem were used in the experiment. All were classified into three groups: solid, medium\textendash solid and hollow stems. There was considerable influence of the cultivar on callus formation and green plant regeneration. The highest efficiency of green plant regeneration (24\%) was observed for the solid-stemmed AC Abbey cultivar. There was no regeneration from the explants of four cultivars: CLTR 7027, Alentejano, Marquis and Bombona. Principal component analysis showed no differences between the cases under observation (callus induction and green plant regeneration) in their response to pre-treatment temperatures (4 and 8\textdegree C). The examination of the effects of various auxin types in the induction medium on callus formation and green plant regeneration revealed that the strongest stimulation of these processes was observed in the C17 medium with 2,4-D and dicamba. The efficiency of callus formation and green plant regeneration was greater in solid-stemmed cultivars than in hollow-stemmed cultivars.},
  file = {E\:\\Zotero\\storage\\XDSQ4A56\\Weigt et al_2016_Solid-stemmed spring wheat cultivars give better androgenic response than.pdf},
  journal = {In Vitro Cellular \& Developmental Biology},
  number = {6},
  pmcid = {PMC5159440},
  pmid = {28042231}
}

