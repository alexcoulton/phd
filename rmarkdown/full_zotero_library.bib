  title = {Arthropod Diversity in a Tropical Forest},
  author = {Basset, Y and Cizek, L and Cuenoud, P and Didham, R K and Guilhaumon, F and Missa, O and Novotny, V and Odegaard, F and Roslin, T and Schmidl, J and Tishechkin, A K and Winchester, N N and Roubik, D W and Aberlenc, H P and Bail, J and Barrios, H and Bridle, J R and {Castano-Meneses}, G and Corbara, B and Curletti, G and {Duarte da Rocha}, W and De Bakker, D and Delabie, J H and Dejean, A and Fagan, L L and Floren, A and Kitching, R L and Medianero, E and Miller, S E and {Gama de Oliveira}, E and Orivel, J and Pollet, M and Rapp, M and Ribeiro, S P and Roisin, Y and Schmidt, J B and Sorensen, L and Leponce, M},
