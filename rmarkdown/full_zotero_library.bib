@article{czech-damalElectroreceptionGuianaDolphin2012,
  title = {Electroreception in the {{Guiana}} Dolphin ({{Sotalia}} Guianensis)},
  author = {{Czech-Damal}, Nicole U. and Liebschner, Alexander and Miersch, Lars and Klauer, Gertrud and Hanke, Frederike D. and Marshall, Christopher D. and Dehnhardt, Guido and Hanke, Wolf},
  year = {2012},
  volume = {279},
  pages = {663--8},
  issn = {1471-2954},
  doi = {10.1098/rspb.2011.1127},
  abstract = {Passive electroreception is a widespread sense in fishes and amphibians, but in mammals this sensory ability has previously only been shown in monotremes. While the electroreceptors in fish and amphibians evolved from mechanosensory lateral line organs, those of monotremes are based on cutaneous glands innervated by trigeminal nerves. Electroreceptors evolved from other structures or in other taxa were unknown to date. Here we show that the hairless vibrissal crypts on the rostrum of the Guiana dolphin (Sotalia guianensis), structures originally associated with the mammalian whiskers, serve as electroreceptors. Histological investigations revealed that the vibrissal crypts possess a well-innervated ampullary structure reminiscent of ampullary electroreceptors in other species. Psychophysical experiments with a male Guiana dolphin determined a sensory detection threshold for weak electric fields of 4.6 \textmu V cm(-1), which is comparable to the sensitivity of electroreceptors in platypuses. Our results show that electroreceptors can evolve from a mechanosensory organ that nearly all mammals possess and suggest the discovery of this kind of electroreception in more species, especially those with an aquatic or semi-aquatic lifestyle.},
  file = {E\:\\Zotero\\storage\\AZX5KG29\\Guyana Dolphin.pdf},
  journal = {Proceedings of The Royal Society - Biological sciences},
  keywords = {dolphin,electroreception,sensory system,vibrissal crypt,vibrissal system,whiskers},
  number = {1729},
  pmid = {21795271}
}

@article{dackeAnimalBehaviourInsect2003,
