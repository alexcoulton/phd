@article{huntIVAAccurateNovo2015,
  title = {{{IVA}}: Accurate de Novo Assembly of {{RNA}} Virus Genomes},
  shorttitle = {{{IVA}}},
  author = {Hunt, Martin and Gall, Astrid and Ong, Swee Hoe and Brener, Jacqui and Ferns, Bridget and Goulder, Philip and Nastouli, Eleni and Keane, Jacqueline A. and Kellam, Paul and Otto, Thomas D.},
  year = {2015},
  month = jul,
  volume = {31},
  pages = {2374--2376},
  issn = {1367-4803},
  doi = {10.1093/bioinformatics/btv120},
  abstract = {Motivation: An accurate genome assembly from short read sequencing data is critical for downstream analysis, for example allowing investigation of variants within a sequenced population. However, assembling sequencing data from virus samples, especially RNA viruses, into a genome sequence is challenging due to the combination of viral population diversity and extremely uneven read depth caused by amplification bias in the inevitable reverse transcription and polymerase chain reaction amplification process of current methods., Results: We developed a new de novo assembler called IVA (Iterative Virus Assembler) designed specifically for read pairs sequenced at highly variable depth from RNA virus samples. We tested IVA on datasets from 140 sequenced samples from human immunodeficiency virus-1 or influenza-virus-infected people and demonstrated that IVA outperforms all other virus de novo assemblers., Availability and implementation: The software runs under Linux, has the GPLv3 licence and is freely available from http://sanger-pathogens.github.io/iva, Contact: iva@sanger.ac.uk, Supplementary information: Supplementary data are available at Bioinformatics online.},
  file = {E\:\\Zotero\\storage\\X96NKVPI\\Hunt et al. - 2015 - IVA accurate de novo assembly of RNA virus genome.pdf},
  journal = {Bioinformatics},
  number = {14},
  pmcid = {PMC4495290},
  pmid = {25725497}
}

