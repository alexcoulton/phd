@article{coolenSocialLearningNoncolonial2005,
  title = {Social Learning in Noncolonial Insects?},
  author = {Coolen, Isabelle and Dangles, Olivier and Casas, J{\'e}r{\^o}me},
  year = {2005},
  volume = {15},
  pages = {1931--1935},
  issn = {09609822},
  doi = {10.1016/j.cub.2005.09.015},
  abstract = {Social-information use has generated great interest lately and has been shown to have important implications for the ecology and evolution of species [1-6]. Learning about predators or predation risk from others may provide low-cost life-saving information and would be expected to have adaptive payoffs in any species where conspecifics are observable and behave differently under predation risk. Yet, social learning and social-information use in general have been largely restricted to vertebrates ([1-3, 5, 7-9], but see [10-16]). Here, we show that crickets adapt their predator-avoidance behavior after having observed the behavior of knowledgeable others and maintain these behavioral changes lastingly after demonstrators are gone. These results point toward social learning, a contingency never shown before in noncolonial insects. We show that these long-lasting changes cannot instead be attributed to long re-emergence times, long-lasting effects of alarm pheromones, or residual odor cues. Our findings imply that social learning is likely much more phylogenetically widespread than currently acknowledged and that reliance on social information is determined by ecological rather than taxonomic constrains [17, 18], and they question the generally held assumption that social learning is restricted to large-brained animals assumed to possess superior cognitive abilities. \textcopyright 2005 Elsevier Ltd All rights reserved.},
  file = {E\:\\Zotero\\storage\\AP5BF7B5\\Coolen (2005)},
  journal = {Current Biology},
  number = {21},
  pmid = {16271870}
}

@article{coombsSignalDetectionTheory1999,
  title = {Signal Detection Theory, Lateral-Line Excitation Patterns and Prey Capture Behaviour of Mottled Sculpin},
  author = {Coombs, Sheryl},
  year = {1999},
  volume = {58},
  pages = {421--430},
  issn = {0003-3472},
  doi = {10.1006/anbe.1999.1179},
  abstract = {The frequency with which blinded mottled sculpin, Cottus bairdi, oriented towards a dipole current source (50-Hz vibrating sphere) was measured as a function of source distance (2\textendash 18 cm) and azimuth (either 0\textdegree{} in front or 90\textdegree{} to the side of the fish). The orienting frequency declined from over 70\% to under 50\% as source distance increased from 4 to 12 cm for both frontal and lateral sources. When response biases (frequency of responding in the absence of the signal) were taken into account with the performance metric d{${'}$}, threshold distances (distances at which d{${'}$} fell to 1) for frontal (12.5 cm) and lateral (11.6 cm) sources were 1.35\textendash 1.45 times the mean standard length of fish used in this study. At distances less than 8 cm, d{${'}$} values were considerably higher (i.e. performance was better) for the lateral source, despite the fact that peak stimulus levels at the fish were twice as high for frontal as for lateral sources at any given distance. Performance differences may be related to differences in spatial excitation patterns, in particular the distribution of opposing pressure gradient directions along the lateral-line system, present for lateral sources, but absent for frontal sources.},
  file = {E\:\\Zotero\\storage\\U92ET66W\\Coombs (1999) - Signal detection theory, lateral-line excitation patterns and prey capture behaviour of mottled sculpin},
  journal = {Animal Behaviour},
  number = {2},
  pmid = {10458893}
}

@article{copenhaverCrossoverInterferenceArabidopsis2002,
  title = {Crossover {{Interference}} in {{Arabidopsis}}},
  author = {Copenhaver, G. P. and Housworth, E. A. and Stahl, F. W.},
  year = {2002},
  month = apr,
  volume = {160},
  pages = {1631--1639},
  issn = {0016-6731, 1943-2631},
  abstract = {The crossover distribution in meiotic tetrads of Arabidopsis thaliana differs from those previously described for Drosophila and Neurospora. Whereas a chi-square distribution with an even number of degrees of freedom provides a good fit for the latter organisms, the fit for Arabidopsis was substantially improved by assuming an additional set of crossovers sprinkled, at random, among those distributed as per chi square. This result is compatible with the view that Arabidopsis has two pathways for meiotic crossing over, only one of which is subject to interference. The results further suggest that Arabidopsis meiosis has {$>$}10 times as many double-strand breaks as crossovers.},
  copyright = {Copyright \textcopyright{} 2002 by the Genetics Society of America},
  file = {E\:\\Zotero\\storage\\4NU5XL9Z\\Copenhaver et al. - 2002 - Crossover Interference in Arabidopsis.pdf;E\:\\Zotero\\storage\\IQRIANJ3\\1631.html},
  journal = {Genetics},
  language = {en},
  number = {4},
  pmid = {11973316}
}

