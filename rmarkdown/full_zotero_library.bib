@article{castagneyrolPlantApparencyOverlooked2013,
  title = {Plant Apparency, an Overlooked Driver of Associational Resistance to Insect Herbivory},
  author = {Castagneyrol, Bastien and Giffard, Brice and P{\'e}r{\'e}, Christelle and Jactel, Herv{\'e}},
  year = {2013},
  volume = {101},
  pages = {418--429},
  issn = {00220477},
  doi = {10.1111/1365-2745.12055},
  abstract = {* Herbivore regulation is one of the services provided by plant diversity in terrestrial ecosystems. It has been suggested that tree diversity decreases insect herbivory in forests, but recent studies have reported opposite patterns, indicating that tree diversity can trigger associational resistance or susceptibility. The mechanisms underlying the tree diversity\textendash resistance relationship thus remain a matter of debate.\$\textbackslash backslash\$n\$\textbackslash backslash\$n\$\textbackslash backslash\$n* We assessed insect herbivory on pedunculate oak saplings (Quercus robur) in a large-scale experiment in which we manipulated tree diversity and identity by mixing oaks, birch and pine species.\$\textbackslash backslash\$n\$\textbackslash backslash\$n\$\textbackslash backslash\$n* Tree diversity at the plot scale had no effect on damage due to leaf chewers, but abundance of leaf miners decreased with increasing tree diversity. The magnitude of this associational resistance increased with host dilution, consistent with the `resource concentration hypothesis'.\$\textbackslash backslash\$n\$\textbackslash backslash\$n\$\textbackslash backslash\$n* At a smaller scale, we estimated tree apparency as the difference in total height between focal oak saplings and their nearest neighbouring trees. Levels of oak infestation with leaf miners decreased significantly with decreasing tree apparency. As the probability of having taller neighbours increased with tree diversity, notably due to the increase in the proportion of faster growing nonhost trees, such as birches and pines, tree apparency may be seen as a `hidden', sampling effect of tree diversity.\$\textbackslash backslash\$n\$\textbackslash backslash\$n\$\textbackslash backslash\$n* Synthesis. These findings suggest that greater host dilution and lower tree apparency contribute to associational resistance in young trees. They also highlight the importance of taking plant size into account as a covariate, to avoid misleading interpretations about the biodiversity\textendash resistance relationship.},
  journal = {Journal of Ecology},
  keywords = {Biodiversity,Forest,Functional diversity,Leaf chewers,Leaf miners,Phylogenetic diversity,Plant-herbivore interactions,Resource concentration,Tree apparency},
  number = {2}
}

