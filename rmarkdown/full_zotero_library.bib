  keywords = {1988b,1992,1996,and marshall-,emulation,horner,imitation,including those by galef,learning have appeared in,litchfield,observational learning,past 20 years,several reviews of observational,social facilitation,stimulus enhancement,the,whiten,whiten and ham,zentall},
  number = {2},
