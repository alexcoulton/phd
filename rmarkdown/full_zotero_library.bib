@article{grachevaGanglionspecificSplicingTRPV12013,
  title = {Ganglion-Specific Splicing of {{TRPV1}} Underlies Infrared Sensation in Vampire Bats},
  author = {Gracheva, Elena O and {Cordero-morales}, Julio F and Nicholas, T and Manno, Carlo and Aranguren, Carla I and Weissman, Jonathan S and Julius, David and Francisco, San and Francisco, San and Francisco, San and Francisco, San},
  year = {2013},
  volume = {476},
  pages = {88--91},
  issn = {1476-4687},
  doi = {10.1038/nature10245.Ganglion-specific},
  abstract = {Vampire bats (Desmodus rotundus) are obligate blood feeders that have evolved specialized systems to suit their unique sanguinary lifestyle 1\textendash 3. Chief among such adaptations is the ability to detect infrared radiation as a means of locating hot spots on warm-blooded prey. Among vertebrates, only vampire bats, boas, pythons, and pit vipers are capable of detecting infrared radiation 1,4. In each case, infrared heat is detected by trigeminal nerve fibers that innervate specialized pit organs on the animal's face 5\textendash 10. Thus, vampire bats and snakes have taken thermosensation to the extreme by developing specialized systems for detecting infrared radiation. As such, these creatures provide a window into the molecular and genetic mechanisms underlying evolutionary tuning of thermoreceptors in a species or cell type specific manner. Previously, we have shown that snakes co-opt a non-heat sensitive channel (vertebrate TRPA1) to produce an infrared detector 6. Here we show that vampire bats tune an already heat sensitive channel (TRPV1) by lowering its thermal activation threshold to {$\sim$}30\textdegree C. This is achieved through alternative splicing of TRPV1 transcripts to produce a channel with a truncated C-terminal cytoplasmic domain. Remarkably, these splicing events occur exclusively in trigeminal ganglia (TG), and not dorsal root ganglia (DRG), thereby maintaining a role for TRPV1 as a detector of noxious heat in somatic afferents. This reflects a unique organization of the bat TRPV1 gene that we show to be characteristic of Laurasiatheria mammals (cows, dogs, and moles), supporting a close phylogenetic relationship with bats. These findings reveal a unique molecular mechanism for physiological tuning of thermosensory nerve fibers.},
  file = {E\:\\Zotero\\storage\\VZSF39Q3\\nature10245.pdf},
  journal = {Nature},
  number = {7358},
  pmid = {21814281}
}

@article{grachevaMolecularBasisInfrared2010,
  title = {Molecular Basis of Infrared Detection by Snakes.},
  author = {Gracheva, Elena O and Ingolia, Nicholas T and Kelly, Yvonne M and {Cordero-Morales}, Julio F and Hollopeter, Gunther and Chesler, Alexander T and S{\'a}nchez, Elda E and Perez, John C and Weissman, Jonathan S and Julius, David},
  year = {2010},
  volume = {464},
  pages = {1006--1011},
  issn = {1476-4687},
  doi = {10.1038/nature08943},
  abstract = {Snakes possess a unique sensory system for detecting infrared radiation, enabling them to generate a 'thermal image' of predators or prey. Infrared signals are initially received by the pit organ, a highly specialized facial structure that is innervated by nerve fibres of the somatosensory system. How this organ detects and transduces infrared signals into nerve impulses is not known. Here we use an unbiased transcriptional profiling approach to identify TRPA1 channels as infrared receptors on sensory nerve fibres that innervate the pit organ. TRPA1 orthologues from pit-bearing snakes (vipers, pythons and boas) are the most heat-sensitive vertebrate ion channels thus far identified, consistent with their role as primary transducers of infrared stimuli. Thus, snakes detect infrared signals through a mechanism involving radiant heating of the pit organ, rather than photochemical transduction. These findings illustrate the broad evolutionary tuning of transient receptor potential (TRP) channels as thermosensors in the vertebrate nervous system.},
  file = {E\:\\Zotero\\storage\\7FS3ZKA4\\snake molecular basis nature.pdf},
  journal = {Nature},
  number = {7291},
  pmid = {20228791}
}

@article{grahamEcologicalRulesGoverning2008,
  title = {Ecological Rules Governing Helminth Microparasite Coinfection},
  author = {Graham, Andrea L},
  year = {2008},
  volume = {105},
  pages = {566--570},
  issn = {0027-8424},
  doi = {10.1073/pnas.0707221105},
  abstract = {Coinfection of a host by multiple parasite species has important epidemiological and clinical implications. However, the direction and magnitude of effects vary considerably among systems, and, until now, there has been no general framework within which to explain this variation. Community ecology has great potential for application to such problems in biomedicine. Here, metaanalysis of data from 54 experiments on laboratory mice reveals that basic ecological rules govern the outcome of coinfection across a broad spectrum of parasite taxa. Specifically, resource-based ("bottom-up") and predator-based ("top-down") control mechanisms combined to determine microparasite population size in helminth-coinfected hosts. Coinfection imposed bottom-up control (resulting in decreased microparasite density) when a helminth that causes anemia was paired with a microparasite species that requires host red blood cells. At the same time, coinfection impaired top-down control of microparasites by the immune system: the greater the helminth-induced suppression of the inflammatory cytokine interferon (IFN)-gamma, the greater the increase in microparasite density. These results suggest that microparasite population growth will be most explosive when underlying helminths do not impose resource limitations but do strongly modulate IFN-gamma responses. Surprisingly simple rules and an ecological framework within which to analyze biomedical data thus emerge from analysis of this dataset. Through such an interdisciplinary lens, predicting the outcome of coinfection may become tractable.},
  file = {E\:\\Zotero\\storage\\HM39GTMS\\PNAS-2008-Graham-566-70.pdf},
  journal = {Proceedings of the National Academy of Sciences},
  number = {2},
  pmid = {18182496}
}

@article{grahamMalariafilariaCoinfectionMice2005,
  title = {Malaria-Filaria Coinfection in Mice Makes Malarial Disease More Severe Unless Filarial Infection Achieves Patency.},
  author = {Graham, Andrea L and Lamb, Tracey J and Read, Andrew F and Allen, Judith E},
  year = {2005},
  volume = {191},
  pages = {410--421},
  issn = {0022-1899},
  doi = {10.1086/426871},
  abstract = {Coinfections are common in natural populations, and the literature suggests that helminth coinfection readily affects how the immune system manages malaria. For example, type 1-dependent control of malaria parasitemia might be impaired by the type 2 milieu of preexisting helminth infection. Alternatively, immunomodulatory effects of helminths might affect the likelihood of malarial immunopathology. Using rodent models of lymphatic filariasis (Litomosoides sigmodontis) and noncerebral malaria (clone AS Plasmodium chabaudi chabaudi), we quantified disease severity, parasitemia, and polyclonal splenic immune responses in BALB/c mice. We found that coinfected mice, particularly those that did not have microfilaremia (Mf(-)), had more severe anemia and loss of body mass than did mice with malaria alone. Even when controlling for parasitemia, malaria was most severe in Mf(-) coinfected mice, and this was associated with increased interferon- gamma responsiveness. Thus, in Mf(-) mice, filariasis upset a delicate immunological balance in malaria infection and exacerbated malaria-induced immunopathology.},
  file = {E\:\\Zotero\\storage\\RDJZXGRB\\J Infect Dis.-2005-Graham-410-21.pdf},
  journal = {The Journal of infectious diseases},
  pmid = {15633101}
}

